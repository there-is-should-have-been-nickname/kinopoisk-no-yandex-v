<Window x:Class="KPNoYandexV.View.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KPNoYandexV.View"
        mc:Ignorable="d"
        Title="StartWindow" Height="300" Width="500">
    <Grid Width="140" Height="40">
        <Button FontFamily="Consolas" FontSize="14" Height="35" Margin="0,-57,0,62" Command="{Binding UserOpenClick}">User</Button>
        <Label FontFamily="Consolas" FontSize="8" Margin="23,-22,23,42" FontStyle="Italic">*Лох трусы в горох</Label>
        <Label FontFamily="Consolas" FontSize="8" Margin="-19,72,-19,-52" FontStyle="Italic">*Гений, миллиардер, плейбой, филантроп</Label>
        <Button FontFamily="Consolas" FontSize="14" Height="35" Margin="0,37,0,-32" Command="{Binding AdminOpenClick}">Admin</Button>
    </Grid>
</Window>
